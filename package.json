{
  "name": "express_starter",
  "version": "1.0.0",
  "description": "ExpressJs Starter with TypeORM and Postgres",
  "main": "build/bin/www/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npx nodemon",
    "start": "cross-env NODE_ENV=production node ./build/bin/www/server.js",
    "build": "rimraf ./build && tsc ",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typeorm": "typeorm-ts-node-commonjs -d ./db/data-source.ts",
    "migration:generate": "npm run typeorm migration:generate ./db/migrations",
    "migration:run": "npm run typeorm migration:run ./db/migrations",
    "migration:revert": "npm run typeorm migration:revert ./db/migrations"
  },
  "keywords": [],
  "author": "Muhammad Rafaadana Mountheira",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "envalid": "^7.3.1",
    "express": "^4.18.1",
    "handlebars": "^4.7.7",
    "helmet": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.0.0",
    "nodemailer": "^6.9.1",
    "pg": "^8.7.3",
    "pg-protocol": "^1.5.0",
    "pino": "^8.1.0",
    "pino-http": "^8.1.0",
    "pino-pretty": "^8.1.0",
    "redis": "^4.6.4",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.6",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@swc/core": "^1.3.57",
    "@swc/helpers": "^0.5.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.35",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.7",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.21",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
